pico-8 cartridge // http://www.pico-8.com
version 27
__lua__
x = 64
y = 64
x_prev = x
y_prev = y
face = 1
step = 1

function _init()
	cls()
	
	palt(1, true) -- dark blue is transparent
	palt(0, false) -- black is visible
end

function _update()
	if (btn(0)) then 
		x -= 1
		face = 1
	end
	if (btn(1)) then 
		x += 1
		face = 0
	end
	if (btn(2)) then y -= 1 end
	if (btn(3)) then y += 1 end
end

function _draw()
	rectfill(0,0,127,127,5)
	
	if (x != x_prev) then
		if (step == 2) then
			step = 1
		else
			step += 1
		end
	end
	
	spr((face*16)+step,x,y)
	printh((face*16)+step)
	x_prev = x
	y_prev = y
end

