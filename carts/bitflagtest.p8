version 27
__lua__
function _init()
	top = 0x40
	btm = 0x80
	both = top+btm
	
	cls()
	map(0,0,0,0,16,16)
	
	first = fget(mget(0,0))
	second = fget(mget(0,8))
	third = fget(mget(0,16))
	
	printh('top  '..(top))
	printh('btm  '..(btm))
	printh('both '..(both))
	printh('topfirst'..(top&first))
	printh('btmfirst'..(btm&first))
	printh('bothfirst'..(both&first))
end
